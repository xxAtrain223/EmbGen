<appendage name='Everything'>
    <include>HeaderOne.hpp</include>
    <include standard='true'>HeaderTwo.hpp</include>
    <variable type='uint8_t' name='VarOne' />
    <variable type='int16_t' name='VarTwo' />
    <setup>
        <code insert='each'>
            VarOne += 1;
            VarTwo -= 1;
        </code>
        <code insert='once'>
            delay(500);
        </code>
        <code insert='each'>
            VarTwo += 1;
            VarOne -= 1;
        </code>
    </setup>
    <loop>
        <code>
            VarOne++;
            VarTwo++;
        </code>
    </loop>
    <command name='get'>
        <return_value type='uint8_t' name='one' />
        <return_value type='int16_t' name='two' />
        <code>
            one = VarOne;
            two = VarTwo;
        </code>
    </command>
    <command name='set'>
        <parameter type='uint8_t' name='one' />
        <parameter type='int16_t' name='two' min='-1024' max='1024' />
        <code>
            VarOne = one;
            VarTwo = two;
        </code>
    </command>
    <command name='zero'>
        <code>
            VarOne = 0;
            VarTwo = 0;
        </code>
    </command>
    <stop command='zero' />
</appendage>